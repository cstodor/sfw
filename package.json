    "@angular/compiler": "^5.0.5",
    "@angular/core": "^5.0.5",
    "@angular/flex-layout": "2.0.0-beta.10-4905443",
    "@angular/forms": "^5.0.5",
    "@angular/http": "^5.0.5",
    "@angular/material": "^5.0.0-rc.3",
    "@angular/platform-browser": "^5.0.5",
    "@angular/platform-browser-dynamic": "^5.0.5",
    "@angular/router": "^5.0.5",
    "@asymmetrik/ngx-leaflet": "^2.5.1",
    "@ngx-translate/core": "^9.0.1",
    "@ngx-translate/http-loader": "^2.0.0",
    "@swimlane/ngx-datatable": "^11.1.5",
    "angular-calendar": "^0.5.0-beta.4",
    "angular-tree-component": "^6.0.0",
    "angularfire2": "5.0.0-rc.5-next",
    "chart.js": "^2.7.1",
    "core-js": "^2.5.1",
    "d3": "^4.12.0",
    "dragula": "^3.7.2",
    "firebase": "^4.7.0",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "leaflet": "^1.2.0",
    "moment": "^2.19.3",
    "ng-animate": "^0.3.1",
    "ng2-charts": "^1.6.0",
    "ng2-dragula": "^1.5.0",
    "ng2-file-upload": "^1.3.0",
    "ng2-validation": "^4.2.0",
    "ngx-perfect-scrollbar": "^5.0.5",
    "ngx-quill": "^2.0.4",
    "rxjs": "^5.5.4",
    "screenfull": "^3.3.2",
    "typescript-formatter": "^7.0.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0-rc.2",
